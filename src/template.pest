spec = {
    SOI ~
    WHITESPACE* ~
    ( word+ | start_date+ | duration+ )+ ~
    WHITESPACE* ~
    EOI
}

start_date = { "{{" ~  WHITESPACE* ~ "start_date" ~ WHITESPACE* ~ "}}" }
duration = { "{{" ~  WHITESPACE* ~ "duration" ~ WHITESPACE* ~ "}}" }

word = { char+ }
char = { ASCII_ALPHANUMERIC+ | custom_punctuation+ | "," | "." }

custom_punctuation = { PUNCTUATION ~ !("{" | "}") }
WHITESPACE = { " " }
